
package company {
	entity Person {
		firstName : String
		lastName : String
		op fullName ( ) : String { firstName + " " + lastName }
		address : Address
		phone: Phone
	}

	entity Phone {
		number: String
	}

	entity Address {
		street : String
		city : String
		country : String
	}
	
	entity Boss extends Person {
	}

	entity Employee extends Person {
	}
}

