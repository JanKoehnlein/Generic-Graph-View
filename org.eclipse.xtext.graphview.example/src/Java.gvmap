import org.eclipse.jdt.core.*

diagram JavaInheritanceDiagram type IType {
	node TypeNode for this {
		label NameLabel for this	
		hidden edge ImplementsEdge for each superInterfaceNames.map[javaProject.findType(it)] {
			=> call TypeNode for this
		} 
		hidden edge ExtendsEdge for javaProject.findType(superclassName) {
			=> call TypeNode for this
		}  unless superclassName == null || superclassName == "java.lang.Object"
	}
}