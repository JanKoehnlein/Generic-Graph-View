import org.eclipse.jdt.core.*

diagram JavaInheritanceDiagram type IType {
	node ClassNode for this {
		label ClassLabel for elementName	
		edge ImplementsEdge for each superInterfaceNames {
			-> ref InterfaceNode for this
		} 
		edge ExtendsEdge for superclassName {
			-> ref SuperClassNode for this
		} 
	}
	node InterfaceNode for each superInterfaceNames {
		label InterfaceLabel for this
	}
	node SuperClassNode for superclassName {
		label SuperCassLabel for this
	}
}